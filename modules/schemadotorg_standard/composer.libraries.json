{
  "name": "drupal/schemadotorg",
  "description": "Provides an opinionated demo of the Schema.org Blueprints.",
  "type": "drupal-module",
  "license": "GPL-2.0-or-later",
  "minimum-stability": "dev",
  "homepage": "https://drupal.org/project/schemadotorg",
  "authors": [
    {
      "name": "Jacob Rockowitz (jrockowitz)",
      "homepage": "https://www.drupal.org/u/jrockowitz",
      "role": "Maintainer"
    }
  ],
  "support": {
    "issues": "https://drupal.org/project/issues/schemadotorg",
    "source": "https://git.drupalcode.org/project/schemadotorg"
  },
  "repositories": {
    "masonry/masonry": {
      "type": "package",
      "package": {
        "name": "masonry/masonry",
        "version": "4.2.2",
        "type": "drupal-library",
        "extra": {
          "installer-name": "masonry"
        },
        "dist": {
          "url": "https://github.com/desandro/masonry/archive/refs/tags/v4.2.2.zip",
          "type": "zip"
        },
        "license": "MIT"
      }
    }
  },
  "require": {
    "drupal/admin_toolbar": "~3.0",
    "drupal/address": "~1.0",
    "drupal/coffee": "~1.0",
    "drupal/config_inspector": "~2.0",
    "drupal/config_rewrite": "~1.0",
    "drupal/consumer_image_styles": "~4.0",
    "drupal/content_browser": "~1.0",
    "drupal/dashboards": "~2.0",
    "drupal/default_content": "~2.0",
    "drupal/devel": "~4.0",
    "drupal/dropzonejs": "~2.0",
    "drupal/embed": "~1.0",
    "drupal/entity_browser": "~2.0",
    "drupal/entity_embed": "~1.0",
    "drupal/entity_usage": "~2.0",
    "drupal/features": "~3.0",
    "drupal/field_group": "~3.0",
    "drupal/flexfield": "~2.0",
    "drupal/gin": "~3.0",
    "drupal/gin_lb": "~1.0",
    "drupal/gin_login": "~1.0",
    "drupal/inline_entity_form": "~1.0",
    "drupal/jsonapi_extras": "~3.0",
    "drupal/openapi_jsonapi": "~3.0",
    "drupal/openapi_ui": "~1.0",
    "drupal/openapi_ui_redoc": "~1.0",
    "drupal/paragraphs": "~1.0",
    "drupal/time_field": "~2.0",
    "drupal/token": "~1.0",
    "drupal/toolbar_language_switcher": "~2.0",
    "masonry/masonry": "*"
  },
  "extra": {
    "composer-exit-on-patch-failure": true,
    "patchLevel": {
      "drupal/core": "-p2"
    },
    "patches": {
      "drupal/core": {
        "Issue #3087632: menu_name max length is too long":  "https://git.drupalcode.org/project/drupal/-/merge_requests/1459.diff",
        "Issue #2764829: EditorConfig file lack settings for markdown files": "https://www.drupal.org/files/issues/2022-06-14/drupal-2764829-21.patch",
        "Issue #3100732: Allow specifying `meta` data on JSON:API objects": "https://www.drupal.org/files/issues/2022-05-15/3100732-33.patch"
      },
      "drupal/address": {
        "Issue #2819251: generateSampleValue": "https://www.drupal.org/files/issues/2022-05-16/address-2819251-6.patch"
      }
    }
  }
}
