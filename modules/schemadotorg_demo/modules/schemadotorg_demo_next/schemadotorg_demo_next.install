<?php

/**
 * @file
 * Installation hooks for the Schema.org BlueprintsDemo Next.js module.
 */

use Drupal\user\Entity\User;

/**
 * Implements hook_install().
 */
function schemadotorg_demo_next_install() {
  // Create next account used for basic authentication preview.
  User::create([
    'name' => 'next',
    'pass' => 'next',
    'mail' => 'next@example.com',
    'roles' => ['schemadotorg_demo_next'],
    'status' => TRUE,
  ])->save();
}
